<h1> Listing Fines </h1>
<%= link_to "New Fine", new_fine_path %>

<table>
  <thead>
    <tr>
      <th> issue_returns_id </th>
      <th> student_id </th>
      <th> amount </th>
      <th> payment_status </th>
      <th colspan="3">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @fines.each do |Fine| %>
      <tr>
        <td><%= fine.issue_returns_id %></td>
        <td><%= fine.student_id %></td>
        <td><%= fine.amount %></td>
        <td><%= fine.payment_status%></td>
        <td><%= link_to 'View', Fine %></td>
        <td><%= link_to 'Edit', edit_fines_path %></td>
         <td><%= link_to('Delete', fines_path, :method => 'delete') %></td>
      </tr>
    <% end %>
  </tbody>
</table>