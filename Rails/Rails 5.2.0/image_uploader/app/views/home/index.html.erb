<%= link_to 'ajax page', ajax_path, remote: true %>
<div id="ajax-content"></div>
<script>

    $.ajax({
     type: "GET",// GET in place of POST
     contentType: "application/json; charset=utf-8",
     url: "/home/ajax",
     dataType: "json",
     success: function (result) {
        //do somthing here
        window.alert(result);
        console.log(result)
     },
     error: function (){
        window.alert("something wrong!");
     }
    });
</script> 